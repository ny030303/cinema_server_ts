    "@types/express": "^4.17.13",
    "@types/node": "12",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^8.2.0",
    "nodemon": "^2.0.15",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
