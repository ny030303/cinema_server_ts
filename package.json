{
  "name": "ts-practice",
  "version": "0.1.0",
  "description": "React Write App (Back)",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "yarn build && nodemon ./dist/app.js",
    "start:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
